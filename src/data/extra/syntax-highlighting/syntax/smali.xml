                <WordDetect attribute="Float" String="NaNF" insensitive="true" context="#stay" />
                <RegExpr attribute="Float" context="#stay" String="\-?\d*(\d+\.|\.\d+|\d+)\d*[eE][\-\+]?\d+[dDfF]?\b" />
                <RegExpr attribute="Float" context="#stay" String="\-?\d*(\d+\.|\.\d+)\d*[dDfF]?" />
                <RegExpr attribute="Float" context="#stay" String="\-?\d+[dDfF]\b" />
                <RegExpr attribute="Float" context="#stay" String="\-?0x&hex;*(\.|&hex;+)&hex;*[pP]\-?\d+[dDfF]?\b" />
                <RegExpr attribute="Hex" context="#stay" String="\-?0x&hex;+[LtTsS]?\b" />
                <RegExpr attribute="Decimal" context="#stay" String="\-?\d+[tTLsS]?\b" />
                <HlCChar attribute="Char" context="#stay" />
                <RegExpr attribute="Char" context="#stay" String="'\\u&hex;{4}'" />
                <DetectChar attribute="String" context="String" char="&quot;" />
            </context>
            <context attribute="Normal Text" lineEndContext="#stay" name="Global">
                <DetectChar attribute="Comment" context="Comment" char="#" />
                <RegExpr attribute="Method" context="#stay" String="\b[\w\$]+\s*(?=\s*>?\s*\()" />
                <RegExpr attribute="Field" context="#stay" String="\b[\w\$]+\s*(?=\:)" />
                <IncludeRules context="Literal" />
                <RegExpr attribute="Built-in" context="#stay" String="(?&lt;=&fieldTypeLookBehind;L)&builtin;" />
                <RegExpr attribute="Built-in" context="#stay" String="(?&lt;=&fieldLookBehind;L)&builtin;" />
                <RegExpr attribute="Built-in" context="#stay" String="(?&lt;=\[L)&builtin;" />
                <RegExpr attribute="Built-in" context="#stay" String="(?&lt;=[&typeIdentifiers;]L)&builtin;" />
                <RegExpr attribute="Data Type" context="#stay" String="(?&lt;=&fieldTypeLookBehind;L)[\w/\$]+" />
                <RegExpr attribute="Data Type" context="#stay" String="(?&lt;=&fieldLookBehind;L)[\w/\$]+" />
                <RegExpr attribute="Data Type" context="#stay" String="(?&lt;=\[L)[\w/\$]+" />
                <RegExpr attribute="Data Type" context="#stay" String="(?&lt;=[&typeIdentifiers;]L)[\w/\$]+" />
                <RegExpr attribute="Type Identifier" context="#stay" String="(?&lt;=&fieldTypeLookBehind;)\[*([&typeIdentifiers;]|L)" />
                <RegExpr attribute="Type Identifier" context="#stay" String="(?&lt;=&fieldLookBehind;)(\[*[&typeIdentifiers;])*\[*([&typeIdentifiers;]|L)" />
                <RegExpr attribute="Type Identifier" context="#stay" String="(?&lt;=[;&typeIdentifiers;\(]\))V" />
                <Detect2Chars attribute="Operator" context="#stay" char="." char1="." />
                <DetectChar attribute="Symbol" context="#stay" char="{" />
                <DetectChar attribute="Symbol" context="#stay" char="}" />
                <DetectChar attribute="Symbol" context="#stay" char="&lt;" />
                <DetectChar attribute="Symbol" context="#stay" char="&gt;" />
                <DetectChar attribute="Symbol" context="#stay" char=":" />
                <DetectChar attribute="Symbol" context="#stay" char=";" />
                <DetectChar attribute="Symbol" context="#stay" char="=" />
                <DetectChar attribute="Symbol" context="#stay" char="(" />
                <DetectChar attribute="Symbol" context="#stay" char=")" />
            </context>
            <context attribute="Normal Text" lineEndContext="Method" name="MethodDeclaration">
                <keyword attribute="Keyword" context="#stay" String="keywords" />
                <IncludeRules context="Global" />
            </context>
            <context attribute="Normal Text" lineEndContext="#stay" name="Method">
                <WordDetect attribute="Attribute" String=".registers" context="#stay" />
