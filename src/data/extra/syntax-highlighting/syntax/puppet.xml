<!DOCTYPE language SYSTEM "language.dtd"
[
  <!ENTITY BareWord  "[a-z][a-zA-Z0-9-_]*">
  <!ENTITY CapWord   "[A-Z][a-zA-Z0-9-_]*">
  <!ENTITY namespace "[a-z][a-z0-9_]*">
]>

